{
  "name": "jquery-time-slider",
  "version": "0.0.0",
  "description": "package.json is only use for building the package. see bower.json for more info",
  "main": "dist/jquery-time-slider.min.js",
	"author": "Niels Holt <niels@steenbuchholt.dk>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^1.9",
    "moment": "^2.9.0"
  },
  "devDependencies": {
    "bourbon": "latest",
    "modernizr-mixin": "latest",
    "mathsass": "latest",
    "jshint": "latest",
    "uglifyjs": "latest"
  },
  "jshintConfig": {
    "globals": {
      "L": false,
      "$": false,
			"jQuery": false,
      "console": false,
      "DEBUG": false,
			"document": false,
			"moment": false,
			"alert"		: false
    },
    "undef": true,
    "unused": true
  },
  "scripts": {
    "lint": "jshint src/jquery-time-slider.js",
		"build:js": "uglifyjs src/jquery-time-slider.js -c -m -o dist/jquery-time-slider.min.js",
    "build": "npm run build:js",
    "prebuild:js": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NielsHolt/jquery-time-slider.git"
  },
  "bugs": {
    "url": "https://github.com/NielsHolt/jquery-time-slider/issues"
  },
  "homepage": "https://github.com/NielsHolt/jquery-time-slider"
}
